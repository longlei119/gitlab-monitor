# Development Environment Variables for GitLab Metrics Backend

# Application
SPRING_PROFILES_ACTIVE=dev
SERVER_PORT=8080

# Database Configuration
DATABASE_URL=jdbc:mysql://localhost:3306/gitlab_metrics?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
DATABASE_USERNAME=gitlab_user
DATABASE_PASSWORD=gitlab_password
DATABASE_SSL_MODE=DISABLED

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0
REDIS_TIMEOUT=2000ms

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=admin123
RABBITMQ_VIRTUAL_HOST=/

# GitLab Webhook Configuration
GITLAB_WEBHOOK_SECRET=dev-webhook-secret
GITLAB_API_TOKEN=dev-gitlab-api-token

# SonarQube Configuration
SONARQUBE_URL=http://localhost:9000
SONARQUBE_TOKEN=dev-sonarqube-token

# JWT Configuration
JWT_SECRET=dev-jwt-secret-key-for-development-only
JWT_EXPIRATION=86400000

# Encryption Configuration
ENCRYPTION_KEY=dev-32-character-encryption-key-dev

# Logging Configuration
LOGGING_LEVEL_ROOT=DEBUG
LOGGING_LEVEL_COM_GITLAB_METRICS=DEBUG
LOGGING_FILE_NAME=logs/gitlab-metrics-dev.log

# JVM Configuration
JAVA_OPTS=-Xms256m -Xmx512m -XX:+UseG1GC

# Monitoring Configuration
MANAGEMENT_ENDPOINTS_WEB_EXPOSURE_INCLUDE=*
MANAGEMENT_ENDPOINT_HEALTH_SHOW_DETAILS=always

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_REQUESTS_PER_MINUTE=1000

# SSL/TLS Configuration
SERVER_SSL_ENABLED=false